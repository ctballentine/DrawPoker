$(".computerWins").text(<%=Game.where(winner: 'computer wins').length%>);
$(".playerWins").text(<%=Game.where(winner: 'player wins').length%>);
$(".computerWinsPercent").text(<%=(Game.where(winner: 'computer wins').length.to_f/Game.where.not(winner: nil).length*100).round(2).to_s %> + "%");
$(".playerWinsPercent").text(<%=(Game.where(winner: 'player wins').length.to_f/Game.where.not(winner: nil).length*100).round(2)%> + "%");
$(".royalFlushNumber").text(<%=Game.where(final_player_hand_rank: "royal flush").length + Game.where(final_computer_hand_rank: "royal flush").length%>);


